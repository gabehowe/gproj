#!/bin/bash
source /home/gabri/dev/database/venv/bin/activate
file_location="$(mktemp)"
python3 /home/gabri/dev/database/make.py $file_location
export tempsearchprojectlocation="$(cat /tmp/searchtempfile)"
cd $tempsearchprojectlocation

if [ -f $tempsearchprojectlocation/start ]; then
	. $tempsearchprojectlocation/start
fi
rm /home/gabri/dev/database/searchtempfile
